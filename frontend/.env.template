# Environment Configuration Template
# Copy this file to .env and fill in the values from your SAM deployment outputs

# AWS Region
VITE_AWS_REGION=eu-central-1

# Cognito Configuration (from SAM outputs)
VITE_COGNITO_USER_POOL_ID=
VITE_COGNITO_USER_POOL_CLIENT_ID=

# API Gateway URL (from SAM outputs)
VITE_API_GATEWAY_URL=

# S3 Bucket Names (from SAM outputs)
VITE_PHOTOS_BUCKET_NAME=
VITE_THUMBNAILS_BUCKET_NAME=

# CloudFront Distribution URL (from SAM outputs)
VITE_CLOUDFRONT_URL=

# Application Configuration
VITE_APP_NAME="Photo Share App"
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,image/webp
