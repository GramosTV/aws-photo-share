    "vite": "^5.0.0",
    "vue-tsc": "^2.1.6",
    "typescript": "^5.6.2",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.17.0",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "autoprefixer": "^10.4.0",
