{
  "name": "aws-photo-share-frontend",
  "version": "1.0.0",
  "description": "Vue.js frontend for AWS Serverless Photo Sharing App",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
